{
    "name": "ytdpnl",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["https://*.youtube.com/*", "https://*.youtu.be/*"],
          "js": ["content-script.js"]
        }
      ],
    "permissions": [
        "https://*.youtube.com/*",
        "https://*.youtu.be/*",
        "https://ytdpnl-dev.fmdj.fr/api/*",
        "https://ytdnlp.fmdj.fr/api/*"
    ]
}
