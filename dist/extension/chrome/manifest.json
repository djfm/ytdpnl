{
	"name": "ytdpnl",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": [
				"https://*.youtube.com/*",
				"https://*.youtu.be/*"
			],
			"js": [
				"content-script.js"
			]
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {},
	"permissions": [
		"webRequest",
		"declarativeNetRequest"
	],
	"host_permissions": [
		"*://*.youtube.com/*",
		"*://*.youtu.be/*"
	]
}